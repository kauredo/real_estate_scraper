<% content_for(:meta_title) { @listing_complex.name } %>
<% content_for(:meta_image) { @listing_complex.main_photo || @listing_complex.listings.first&.photos&.first } %>
<div id="show" class="relative">
  <% if @listing_complex.new_format %>
    <%= react_component 'listingComplex--NewShow', { complex: @listing_complex_json }  %>
  <% else %>
    <%= react_component 'listingComplex--Show', { complex: @listing_complex_json }  %>
  <% end %>
</div>
