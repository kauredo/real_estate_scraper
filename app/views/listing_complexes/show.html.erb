<% content_for(:meta_title) { @listing_complex.name } %>
<% content_for(:meta_image) { @listings.first.photos.first } %>
<% if @listing_complex.new_format %>
  <%= react_component 'listingComplex--NewShow', { complex: @listing_complex, listings: @listings }  %>
<% else %>
  <%= react_component 'listingComplex--Show', { complex: @listing_complex, listings: @listings }  %>
<% end %>
