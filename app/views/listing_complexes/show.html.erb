<% content_for(:meta_title) { @listing_complex.name } %>
<% content_for(:meta_image) { @listing_complex.main_photo || @listing_complex.listings.first&.photos&.first } %>
<div id="show" class="relative">
  <% if current_admin.present? && current_admin.confirmed? %>
    <div class="absolute" style="z-index: 99; top: 3rem; left: 2rem">
      <%= link_to 'Ver Empreendimento no Backoffice', edit_backoffice_listing_complex_path(@listing_complex), class: 'bg-blue-300 hover:bg-blue-500 text-white font-bold py-2 px-4 rounded focus:outline-none focus:shadow-outline' %>
    </div>
  <% end %>
  <% if @listing_complex.new_format %>
    <%= react_component 'listingComplex--NewShow', { complex: @listing_complex_json }  %>
  <% else %>
    <%= react_component 'listingComplex--Show', { complex: @listing_complex_json }  %>
  <% end %>
</div>
